{
    "name": "apollo-node-backend",
    "version": "0.0.1",
    "description": "",
    "author": "Pradeep Singh",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "start:dev": "NODE_ENV=development nest start --watch",
        "start:stage": "NODE_ENV=staging node dist/main",
        "start:prod": "NODE_ENV=production node dist/main",
        "start:debug": "nest start --debug --watch",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "prepare": "husky install",
        "migrate:up": "dotenv -e .env.${NODE_ENV} npx sequelize-cli db:migrate",
        "migrate:undo": "dotenv -e .env.${NODE_ENV} npx sequelize-cli db:migrate:undo"
    },
    "dependencies": {
        "@nestjs/cache-manager": "^2.1.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/platform-express": "^10.4.1",
        "@nestjs/sequelize": "^10.0.0",
        "@nestjs/swagger": "^7.1.16",
        "@nestjs/throttler": "^4.2.1",
        "aws-sdk": "^2.1688.0",
        "axios": "^1.4.0",
        "cache-manager": "^5.3.1",
        "cache-manager-redis-yet": "^4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "dotenv-cli": "^7.4.2",
        "eslint-plugin-unused-imports": "^3.0.0",
        "form-data": "^4.0.0",
        "image-size": "^1.1.1",
        "ioredis": "^5.3.2",
        "ms": "^2.1.3",
        "multer": "^1.4.5-lts.1",
        "mysql": "^2.18.1",
        "mysql2": "^3.6.5",
        "nest-winston": "^1.9.1",
        "payu": "^0.0.1",
        "payu-sdk-test": "github:payu-intrepos/web-sdk-nodejs",
        "payu-websdk": "^1.0.0",
        "pg": "^8.11.2",
        "redis": "^4.6.11",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "sequelize": "^6.32.1",
        "sequelize-typescript": "^2.1.5",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/aws-sdk": "^2.7.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.17",
        "@types/image-size": "^0.8.0",
        "@types/jest": "^29.5.2",
        "@types/ms": "^0.7.31",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.3.1",
        "@types/sequelize": "^4.28.15",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "lint-staged": "^15.2.0",
        "prettier": "^2.8.8",
        "sequelize-cli": "^6.6.2",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
